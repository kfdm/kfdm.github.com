---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>KungFuDiscoMonkey.net</title>
	<link href="http://kungfudiscomonkey.net/atom.xml" rel="self"/>
	<link href="http://kungfudiscomonkey.net/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://kungfudiscomonkey.net/</id>
	<author>
		<name>Paul Traylor</name>
	</author>
{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="http://kungfudiscomonkey.net{{ post.url }}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://kungfudiscomonkey.net{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
{% endfor %}

</feed>